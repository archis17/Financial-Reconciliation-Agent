{
  "id": null,
  "uid": "financial-reconciliation",
  "title": "Financial Reconciliation Dashboard",
  "tags": ["reconciliation", "api"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Reconciliation Success Rate",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(reconciliation_total{status=\"completed\"}[5m]) / rate(reconciliation_total[5m])",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 2,
      "title": "Total Reconciliations",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "sum(increase(reconciliation_total[1h]))",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "Reconciliation Processing Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(reconciliation_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        },
        {
          "expr": "histogram_quantile(0.50, rate(reconciliation_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 4,
      "title": "API Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(api_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "id": 5,
      "title": "API Response Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        },
        {
          "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 6,
      "title": "User Logins",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum(increase(user_logins_total[1h]))",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 7,
      "title": "LLM Calls",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 16},
      "targets": [
        {
          "expr": "sum(increase(llm_calls_total[1h]))",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "LLM Tokens Used",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "sum(increase(llm_tokens_total[1h]))",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 9,
      "title": "Reconciliation Status",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
      "targets": [
        {
          "expr": "sum by (status) (reconciliation_total)",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "prometheus"}
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {}
}
